{
  "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "indent": [2, 2],
      "linebreak-style": 0,
      "quotes": [2, "single"],
      "semi": [2, "always"],
      "no-trailing-spaces": [2]
    },
    "globals": {
      "Phaser": true
    }
  }